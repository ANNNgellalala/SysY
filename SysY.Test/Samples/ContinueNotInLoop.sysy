// 冒泡排序函数
void BubbleSort(int arr[], int n) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - 1 - i; j++) {
            if (arr[j] > arr[j + 1]) {
                // 交换相邻元素
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
    continue; // 这里的 continue 不在循环中
}

int main() {
    int arr[] = {5, 2, 9, 1, 5, 6};
    int n = 6;
    BubbleSort(arr, n);
    return 0;
}